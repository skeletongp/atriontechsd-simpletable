{
    "name": "atriontechsd/simpletable",
    "description": "Create and manage your simple and beautyfull datatable for laravel projects with Tailwindcss and Livewire",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Atriontechsd\\SimpleTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Atriontechsd\\SimpleTable\\":"vendor/atriontechsd/simpletable/src/"
        },
        "files": [
            "src/Helpers/Universal.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atriontechsd\\Simpletable\\Providers\\SimpletableServiceProvider"
            ]
        }
    },
    
    "authors": [
        {
            "name": "Atriontech Soluciones Digitales",
            "email": "support@atriontechsd.com"
        }
    ],
    "require": {
        "laravel/framework": "^9.19",
        "livewire/livewire": "^2.10"
    }
}
